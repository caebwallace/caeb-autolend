{
    "presets": [
        "@babel/preset-env"
    ],
    "plugins": [
        "@babel/plugin-transform-runtime",
        [
            "module-resolver",
            {
                "root": ["./src"],
                "alias": {
                    "@helpers": "./src/helpers",
                    "@notify": "./src/notify",
                    "@mock": "./mock",
                    "@providers": "./src/providers"
                }
            }
        ]
    ]
}
